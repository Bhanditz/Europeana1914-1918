	text-align:	center;
	width:		100%;
}



.nav>div.menu, .nav>div.pagination{
	white-space:	nowrap;
}



.nav .of{
	display:none;
}

.nav .of-bracket{
	color:#333 !important;	/* fix for strange iphone bug (wanting to color this as a link) */
}

.nav .of-bracket:before{
	content:'(';
}

.nav .of-bracket:after{
	content:')';
}

.nav .of-value{
	display:	block;
}

.nav .spaced:before{
	content:'';
}

.nav .spaced:after{
	content:'';
}




/* data showing / hiding rules */

@media only screen and (min-width: 22em) {
	.nav .of-bracket:before{
		content:'';
	}

	.nav .of-bracket:after{
		content:'';
	}

	.nav .of,
	.nav .of-value{
		display:inline;
	}	
}


@media only screen and (min-width: 48em) {
	
	.nav .spaced:before{
		content:' ';
	}
	
	.nav .spaced:after{
		content:' ';
	}
	
	.result-pagination li{
		vertical-align:	middle;
	}
	
	.jump-to-page{
		margin-top:	0;
	}
}


@media only screen and (max-width: 55em) {
	.nav>div.show-on-22{
		display:none;
	}
}

/* desktop general */
@media only screen and (min-width: 55em) {

	.show-on-22{display:table-cell !important;}
	
	.result-pagination{
		float:		right;
		width:		auto;	
		font-size:	1em;
		width:		auto;
	}


	/* flex-box rules */
	.nav>div{
		display:		table-cell;
		vertical-align:	middle;
	}

	.nav>.menu, .nav>.pagination{
		width:1px;
	}

	.nav>.count{
		width:		100%;
		text-align:	center;
	}

	.nav-numbers{	
		font-size:	1em;
		width:		auto;
	}

	.nav>.menu{
		text-align:	left;
	}

	.nav>.pagination{
		text-align:	right;
	}

}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    /* display: none; <- Crashes Chrome on hover */
    -webkit-appearance: none;
    margin: 0;
}


